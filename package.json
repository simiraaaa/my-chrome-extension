{
  "name": "my-chrome-extension",
  "version": "0.4.0",
  "description": "my chrome extension",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simiraaaa/my-chrome-extension.git"
  },
  "author": "simiraaaa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simiraaaa/my-chrome-extension/issues"
  },
  "homepage": "https://github.com/simiraaaa/my-chrome-extension#readme",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check ."
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.1",
    "@types/chrome": "^0.1.1",
    "less": "^4.4.0",
    "meltline": "^0.1.17",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "^0.3.2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.4"
  },
  "keywords": [
    "svelte"
  ]
}